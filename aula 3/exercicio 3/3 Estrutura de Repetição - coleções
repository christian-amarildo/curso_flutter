import 'dart:io';

void main() {
  print("Digite o preço máximo dos produtos:");
  int precoMaximo = int.parse(stdin.readLineSync()!);

  List <int> precos = gerarPrecosDisponiveis(precoMaximo);

  print("Preços disponíveis dos produtos: $precos");
}

List<int> gerarPrecosDisponiveis(int precoMaximo) {
  List<int> precos = List.generate(precoMaximo + 1, (index) => index * 2);
  return precos.where((preco) => preco <= precoMaximo).toList();
}
